{
  "id": null,
  "uid": "apex-operational-overview",
  "title": "APEX Operational Overview",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "id": 1,
      "title": "HTTP Requests / sec",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "sum(rate(apex_api_http_requests_total[5m])) by (status)",
          "legendFormat": "status={{status}}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 2,
      "title": "P95 HTTP Latency (s)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(apex_api_http_request_duration_seconds_bucket[5m])) by (le, path))",
          "legendFormat": "{{path}}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 3,
      "title": "In-Flight HTTP Requests",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "targets": [
        {
          "expr": "sum(apex_api_http_requests_in_progress)",
          "legendFormat": "in-flight",
          "refId": "A"
        }
      ]
    },
    {
      "id": 4,
      "title": "WebSocket Connections",
      "type": "stat",
      "gridPos": { "h": 8, "w": 6, "x": 12, "y": 8 },
      "targets": [
        {
          "expr": "apex_api_websocket_connections",
          "refId": "A"
        }
      ]
    },
    {
      "id": 5,
      "title": "WebSocket Messages / sec",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 6, "x": 18, "y": 8 },
      "targets": [
        {
          "expr": "sum(rate(apex_api_websocket_messages_total[5m])) by (direction)",
          "legendFormat": "{{direction}}",
          "refId": "A"
        }
      ]
    }
  ]
}
